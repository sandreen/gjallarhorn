<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        tools:showIn="@layout/activity_storage_test"
        tools:context=".StorageTest">

    <Button
            android:text="Button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/button2" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="298dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="161dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="329dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="162dp"
            android:onClick="upload" app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintVertical_bias="1.0"/>
    <Button
            android:text="Button2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/button3"
            android:onClick="findTune" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="162dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="161dp"
            android:layout_marginBottom="281dp" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/button2" android:layout_marginTop="8dp"/>
    <com.google.android.material.textfield.TextInputLayout
            android:layout_width="395dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/button3" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp">

        <com.google.android.material.textfield.TextInputEditText
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="type here u bitch" android:id="@+id/testInput"/>
    </com.google.android.material.textfield.TextInputLayout>
    <Button
            android:text="Input btn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/button4" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/button2" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:onClick="inputTest"/>
    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="395dp"
            android:layout_height="226dp" tools:layout_editor_absoluteY="8dp" tools:layout_editor_absoluteX="8dp"
            android:id="@+id/fragContainer">
    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>